#include<iostream>
using namespace std;

int main()
{
     
     
        vector<int>nums;
        int k;
        cin>>k;
        make_heap(nums.begin(),nums.end());
        
        for(int i=0;i<k-1;i++)
        {
            pop_heap(nums.begin(),nums.end());
            nums.pop_back();
        }
        

        return nums.front();

}
