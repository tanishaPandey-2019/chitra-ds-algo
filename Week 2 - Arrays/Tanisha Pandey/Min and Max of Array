#include<iostream>
#include<vector>
using namespace std;

int main()
{
	vector<int>arr;
	int temp,n;
	cin >> n;

	int maxNum = INT_MAX;
	int minNum = INT_MIN;

	for (int i = 0; i < n; i++)
	{
		cin >> temp;
		arr.push_back(temp);
		
		if (arr[i] < minNum)
		{
			minNum = arr[i];
		}
		else
		{
			maxNum = arr[i];
		}
	}

	cout << "maximum" << maxNum << endl;
	cout << "minimum" << minNum << endl;

	return 0;
}
